programa {
  funcao inicio() {
    inteiro A = 5
    inteiro B = 5
    inteiro C = 7
    
    se (A == B)  
{  
    escreva ("A é igual a B \n") 
}  
    se (A > B)  
{  
    escreva ("A é maior que B \n") 
}  
    se (A >= B)  
{  
    escreva ("A é maior ou igual que B \n") 
}  
    se (C < B)  
{  
    escreva ("C é menor que B \n") 
}  
    se (C != B)  
{  
    escreva ("C é diferente de B \n") 
}  
  }
}
